{
  "manifest_version": 2,
  "name": "NEW TAB 2 APPS",
  "version": "1.0.0",

  "default_locale": "ja",
  "description": "Chromeの新規タブを、アプリにするだけの拡張機能です",
  "icons": {
    "16": "ico/16x16.png",
    "128": "ico/128x128.png"
  },
  "background": {
      "scripts": [
          "src/background.js"
      ]
  },
  "permissions": [
    "tabs"
  ],
  "short_name": "NEW TAB 2 APPS"
}